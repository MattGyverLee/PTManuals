<fo:list-item relative-align="baseline"><fo:list-item-label end-indent="label-end()"><fo:block>•</fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:external-graphic scaling="uniform" src="url(12imgeng/image25.png)"/></fo:block></fo:list-item-body></fo:list-item>
