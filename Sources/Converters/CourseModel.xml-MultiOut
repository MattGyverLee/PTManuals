               <section1 id="sSetupAdditionalNoteTypes" showinlandscapemode="no">
                  <titleContentChoices>
                     <titleContent contentType="en">Setup additional note types</titleContent>
                     <titleContent contentType="fr">Définir des types de remarques</titleContent>
                  </titleContentChoices>
                  <p contentType="en" pagecontrol="letFormatterDecide">(One must be an Administrator)</p>
                  <p contentType="fr" pagecontrol="letFormatterDecide">(on doit être un Administrateur)</p>
                  <ul>
