<fo:list-item relative-align="baseline" space-after="3pt"><fo:list-item-label end-indent="label-end()"><fo:block>•</fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Click <fo:inline font-weight="bold">Manage plans]</fo:inline></fo:block></fo:list-item-body></fo:list-item>
<fo:list-item relative-align="baseline" space-after="3pt"><fo:list-item-label end-indent="label-end()"><fo:block>•</fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Click on the last stage<fo:block/><fo:external-graphic scaling="uniform" src="url(supimgeng\image22.png)"/></fo:block></fo:list-item-body></fo:list-item>
